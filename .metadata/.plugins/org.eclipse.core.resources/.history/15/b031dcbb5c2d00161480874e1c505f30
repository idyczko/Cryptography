
public class Main {

	public static void main(String[] args) {
		DiningCryptographers diningCryptographers = new DiningCryptographers(5, Topography.valueOf("RING"));
		for(int i =0;i<diningCryptographers.getNodes();i++){
			System.out.println();
			for(int j = 0; j<diningCryptographers.getNodes();j++){
				System.out.print(diningCryptographers.getNetwork()[i][j]+" ");
			}
		}
	}

}
