
public class CustomMathLib {

	public static int modulo(int a, int b){
		return a>0?a%b:(b-Math.abs(a)%b)%b;
	}
	
	public static int xor(int a, int b){
		if(a==b){
			return 0;
		}
		return 1;
	}
	
	public static int xor(int tab[]){
		int xorResult = tab[0];
		for(int i : tab){
			xorResult = CustomMathLib.xor(xorResult,i);
		}
		return xorResult;
	}
	
	public static int negxor(int tab[]){
		int xorResult = 0;
		for(int i : tab){
			xorResult = CustomMathLib.negxor(xorResult,i);
		}
		return xorResult;
	}
	
	public static int negxor(int a, int b){
		if(a==b){
			return 1;
		}
		return 0;
	}
}
